{
  "name": "@holytiny/inquirer",
  "version": "0.0.0",
  "description": "A cli to init project vars",
  "main": "./lib/index.js",
  "bin": {
    "inquirer": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "scripts": {
    "start": "launch",
    "pretest": "rm -rf test/dist/*",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "preversion": "npm test",
    "version": "oclif-dev readme && git add README.md",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holytiny/inquirer.git"
  },
  "keywords": [
    "gitops",
    "init",
    "environment",
    "variable"
  ],
  "author": "sake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holytiny/inquirer/issues"
  },
  "homepage": "https://github.com/holytiny/inquirer#readme",
  "dependencies": {
    "tslog": "2.11.1"
  },
  "devDependencies": {
    "script-launcher": "1.30.0",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
